#include <stdio.h>
#include <stdlib.h>

int main()
{
    int sobrinha;

    printf("Se a melhor sobrinha for a Eloa digite 1, se for a Isabella digite 2, se for a Gabriela digite 3\n");
    scanf("%d", &sobrinha);

    if (sobrinha == 1)
    {
        printf("Eloa e a melhor mesmo!!!\n");
    }

    else if (sobrinha != 1)
    {
        printf("Voce errou, Eloa e a melhor!!!");
    }

    return 0;
}